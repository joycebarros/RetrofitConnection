<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.retrofitconnection">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.UpdateCourseActivity"
            android:label="Tela de Atualização"/>
        <activity
            android:name=".view.AlocacaoActivity"
            android:label="Lista de Alocações" />
        <activity
            android:name=".view.CourseActivity"
            android:label="Lista de Cursos" />
        <activity
            android:name=".view.MainActivityMenu"
            android:label="Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.ProfessorActivity"
            android:label="Lista de Professores" />
        <activity android:name=".view.CreateProfessorActivity"
            android:label="Cadastro de Professor" />
        <activity
            android:name=".view.DepartamentoActivity"
            android:label="Lista de Departamentos" />
        <activity
            android:name=".view.CreateCourseActivity"
            android:label="Cadastro de Curso" />
    </application>

</manifest>